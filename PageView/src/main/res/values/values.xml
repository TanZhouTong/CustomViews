<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <dimen name="font_size_page_indecator">15sp</dimen>
    <dimen name="padding_40">40dp</dimen>
    <dimen name="padding_8">8dp</dimen>
    <color name="black">#ff000000</color>
    <color name="gray">#ff999999</color>
    <color name="white">#ffffffff</color>
    <color name="opacity_gray">#99666666</color>
    <declare-styleable name="PageGridStyleable" ns1:ignore="ResourceName">
        <attr name="rowCount" format="integer" />
        <attr name="columnCount" format="integer" />
        <attr name="looper" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ZYInkBasePagerIndicator" ns1:ignore="ResourceName">
        <attr name="showIndicatorButton" format="boolean" />
        <attr name="indicatorButtonPadding" format="dimension" />
        <attr name="previousDrawable" format="reference" />
        <attr name="nextDrawable" format="reference" />
        <attr name="showWhenOnePage" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ZYInkTextPagerIndicator" ns1:ignore="ResourceName">
        <attr name="indicatorLayout" format="reference" />
    </declare-styleable>

    <declare-styleable name="FlexibleGridView">
        <!--<attr name="rows" format="integer" />   &lt;!&ndash;默认行数&ndash;&gt;
        <attr name="columns" format="integer" /> &lt;!&ndash;默认列数&ndash;&gt;-->
        <attr name="coverWidth" format="dimension" />
        <attr name="coverHeight" format="dimension" />
        <attr name="itemHorizontalMinGap" format="dimension" /> <!--水平方向上最小的gap-->
        <attr name="itemVerticalMinGap" format="dimension" />   <!--垂直方向上最小的gap-->
        <attr name="circle" format="boolean" /> <!--翻页循环-->
        <attr name="corner" format="dimension" /> <!--标题部分textSize-->
        <attr name="rtSize" format="dimension" /> <!--右上部分元素的大小-->
        <attr name="rtMarginTop" format="dimension" /> <!--右上部分元素对上边界的margin-->
        <attr name="rtMarginRight" format="dimension" /> <!--右上部分元素对右边界的margin-->
        <attr name="rbTextSize" format="dimension" /> <!--右下部分textSize-->
        <attr name="rbMarginHorizontal" format="dimension" /> <!--右下部分对于所在rect的左边距-->
        <attr name="rbMarginVertical" format="dimension" /> <!--右下部分对于所在rect的右边距-->
        <attr name="titleTextSize" format="dimension" /> <!--标题部分textSize-->
    </declare-styleable>
</resources>